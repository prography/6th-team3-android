<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.RegisterFragment"
    tools:layout_editor_absoluteX="20dp"
    tools:layout_editor_absoluteY="226dp">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="사용자 정보 입력"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_register_next_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="다음"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_register_previous_screen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_register_previous_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이전화면"
        app:layout_constraintBottom_toBottomOf="@+id/btn_register_next_screen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_register_next_screen"
        app:layout_constraintTop_toTopOf="@+id/btn_register_next_screen" />

    <EditText
        android:id="@+id/et_register_password_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/tv_register_password_check"
        app:layout_constraintEnd_toEndOf="@+id/et_register_phone_number"
        app:layout_constraintTop_toTopOf="@+id/tv_register_password_check" />

    <EditText
        android:id="@+id/et_register_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/tv_register_name"
        app:layout_constraintEnd_toEndOf="@+id/et_register_email"
        app:layout_constraintTop_toTopOf="@+id/tv_register_name" />

    <TextView
        android:id="@+id/tv_register_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="128dp"
        android:text="이름/닉네임"
        app:layout_constraintEnd_toStartOf="@+id/et_register_name"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_register_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/tv_register_password"
        app:layout_constraintEnd_toEndOf="@+id/et_register_password_check"
        app:layout_constraintTop_toTopOf="@+id/tv_register_password" />

    <EditText
        android:id="@+id/et_register_phone_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_register_phone_number"
        app:layout_constraintTop_toBottomOf="@+id/et_register_password_check" />

    <TextView
        android:id="@+id/tv_register_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="232dp"
        android:text="이메일"
        app:layout_constraintEnd_toStartOf="@+id/et_register_email"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_register_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/tv_register_email"
        app:layout_constraintEnd_toEndOf="@+id/et_register_password"
        app:layout_constraintTop_toTopOf="@+id/tv_register_email" />

    <TextView
        android:id="@+id/tv_register_phone_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="휴대폰번호"
        app:layout_constraintBottom_toBottomOf="@+id/et_register_phone_number"
        app:layout_constraintEnd_toStartOf="@+id/et_register_phone_number"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/et_register_phone_number" />

    <TextView
        android:id="@+id/tv_register_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="78dp"
        android:text="비밀번호"
        app:layout_constraintEnd_toStartOf="@+id/et_register_password"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_register_email" />

    <TextView
        android:id="@+id/tv_register_password_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="78dp"
        android:text="비밀번호확인"
        app:layout_constraintEnd_toStartOf="@+id/et_register_password_check"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_register_password" />

</androidx.constraintlayout.widget.ConstraintLayout>