<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false"
        android:state_enabled="true">
        <shape
            android:shape="rectangle">
                <stroke
                android:color="@color/petHotelSecondaryDark"
                android:width="1dp"/>
                <solid
                android:color="@android:color/white" />
                <corners
                android:radius="25dp"
                />
            <padding
                android:right="20dp"
                android:left="20dp"
                />

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle"
            >
            <solid
                android:color="@color/petHotelSecondaryDark"/>
            <corners
                android:radius="25dp"
                />
            <padding
                android:right="20dp"
                android:left="20dp"
                />

        </shape>
    </item>

</selector>